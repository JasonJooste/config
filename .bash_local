# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/joo007/miniconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
